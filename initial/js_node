
export function countInversion(sequence: number[]): number{
    return 0;
}

import * as assert from 'assert';

console.log("Example:");
console.log(countInversion([1, 2, 5, 3, 4, 7, 6]));

assert.equal(countInversion([1, 2, 5, 3, 4, 7, 6]), 3, "Example");
assert.equal(countInversion([0, 1, 2, 3]), 0);
assert.equal(countInversion([99, -99]), 1);
assert.equal(countInversion([5, 3, 2, 1, 0]), 10);
